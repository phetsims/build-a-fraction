{
  "name": "build-a-fraction",
  "version": "1.1.0-dev.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/phetsims/build-a-fraction.git"
  },
  "devDependencies": {
    "grunt": "~1.5.3"
  },
  "phet": {
    "requirejsNamespace": "BUILD_A_FRACTION",
    "phetLibs": [
      "fractions-common",
      "twixt",
      "vegas"
    ],
    "runnable": true,
    "supportedBrands": [
      "phet",
      "adapted-from-phet"
    ],
    "simFeatures": {
      "supportsSound": true
    },
    "simulation": true,
    "supportsOutputJS": true,
    "published": true,
    "screenNameKeys": [
      "BUILD_A_FRACTION/screen.buildAFraction",
      "BUILD_A_FRACTION/screen.mixedNumbers",
      "BUILD_A_FRACTION/screen.lab"
    ]
  },
  "eslintConfig": {
    "extends": "../chipper/eslint/sim_eslintrc.js",
    "overrides": [
      {
        "files": [
          "**/*.ts"
        ],
        "parserOptions": {
          "project": [
            "../chipper/tsconfig/eslint-all/tsconfig.json"
          ]
        }
      }
    ]
  }
}